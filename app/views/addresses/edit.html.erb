<h2>Edit Address</h2>
<%= form_for [current_user, @address] do |f| %>
  <p><%= f.label :address %>
  <%= f.text_field :address %></p>

  <p><%= f.label :city %>
  <%= f.text_field :city %></p>

  <p><%= f.label :state %>
  <%= f.text_field :state %></p>

  <p><%= f.label :zip %>
  <%= f.text_field :zip %></p>

  <p><%= f.label :nickname %>
  <%= f.text_field :nickname %></p>

  <p><%= f.submit 'Edit' %></p>
<% end %>
